{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ShipOption": "newrelic",
  "UseLogging": true,
  "Jaeger": {
    "ServiceName": "TestApp",
    "AgentHost": "localhost",
    "AgentPort": 6831
  },
  "NewRelic": {
    "ApiKey": "#{NewRelicAPIKey}#",
    "ServiceName": "#{ServiceName}#"
  },
  "AspNetCoreInstrumentation": {
    "RecordException": true
  },
  "AllowedHosts": "*"
}
